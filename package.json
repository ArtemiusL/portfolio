{
  "name": "react-starter-boilerplate",
  "version": "2.3.1",
  "description": "A starter boilerplate for an universal web application with the best development experience and best practices.",
  "main": "index.js",
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snphq/react-starter-boilerplate"
  },
  "keywords": [
    "starter",
    "boilerpate",
    "universal",
    "react",
    "redux",
    "react router v4",
    "express",
    "webpack v3",
    "es6",
    "code splitting",
    "hot reloading",
    "babel",
    "postcss",
    "performance optimizing"
  ],
  "license": "MIT",
  "scripts": {
    "start": "better-npm-run start",
    "start:static": "yarn build && better-npm-run start:static",
    "start:production": "yarn build && yarn start:prod",
    "start:prod": "better-npm-run start:prod",
    "clean:build": "better-npm-run clean:build",
    "build": "yarn clean:build && better-npm-run build",
    "build:static": "yarn clean:build && better-npm-run build:assets && better-npm-run build:static",
    "analyze": "yarn clean:build && better-npm-run analyze",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:js": "better-npm-run lint:js",
    "lint:style": "better-npm-run lint:style",
    "deploy": "mkdir -p shipit-syncing-source & shipit staging deploy -e production"
  },
  "betterScripts": {
    "start": {
      "command": "node ./dev.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start:static": {
      "command": "node ./static.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start:prod": {
      "command": "node ./dev.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "build": {
      "command": "webpack --hide-modules --config ./tools/webpack/config.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:assets": {
      "command": "webpack --hide-modules --config ./tools/webpack/config.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:static": {
      "command": "node ./tools/static-build/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "analyze": {
      "command": "webpack --progress --hide-modules --config ./tools/webpack/config.babel.js",
      "env": {
        "NODE_ENV": "analyze"
      }
    },
    "lint:js": {
      "command": "eslint --fix ./src ./tools ./dev.js"
    },
    "lint:style": {
      "command": "stylelint --fix \"./src/**/*.scss\""
    },
    "clean:build": {
      "command": "rimraf ./public/*"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage"
        }
      ],
      "@babel/preset-react",
      [
        "@babel/preset-stage-0",
        {
          "decoratorsLegacy": true
        }
      ]
    ],
    "plugins": [
      "dynamic-import-node",
      "lodash",
      [
        "module-resolver",
        {
          "alias": {
            "_constants": "./src/constants",
            "_actionConstants": "./src/actions/constants",
            "_actions": "./src/actions",
            "_api": "./src/api",
            "_hocs": "./src/hocs",
            "_utils": "./src/utils",
            "_sagas": "./src/sagas",
            "_config": "./src/config",
            "_reducers": "./src/reducers",
            "_store": "./src/store",
            "_pages": "./src/pages",
            "_containers": "./src/containers",
            "_components": "./src/components",
            "_styles": "./src/styles",
            "_images": "./src/assets/images",
            "_localization": "./src/localization",
            "_selectors": "./src/selectors"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "plugin:lodash-fp/recommended",
      "airbnb"
    ],
    "plugins": [
      "lodash-fp",
      "react"
    ],
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./tools/webpack/config.babel.js"
        }
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "lodash-fp/no-unused-result": "off",
      "global-require": "off",
      "no-console": "off",
      "no-underscore-dangle": "off",
      "function-paren-newline": "off",
      "import/first": "off",
      "import/prefer-default-export": "off",
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "specialLink": [
            "to"
          ]
        }
      ],
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "lodash-fp/use-fp": "off",
      "lodash-fp/consistent-name": "off",
      "react/forbid-prop-types": "off",
      "react/require-default-props": "off"
    },
    "globals": {
      "__CLIENT__": true,
      "__SERVER__": true,
      "__DEV__": true
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard"
    ],
    "rules": {
      "string-quotes": "single",
      "at-rule-no-unknown": null,
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global",
            "local"
          ]
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "@babel/register": "^7.0.0-beta.46",
    "add-zero": "^1.0.0",
    "axios": "^0.18.0",
    "bluebird": "^3.5.2",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "final-form": "^4.10.0",
    "helmet": "^3.12.0",
    "history": "^4.7.2",
    "hpp": "^0.2.2",
    "http-proxy-middleware": "^0.19.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.0",
    "normalize.css": "^8.0.0",
    "pretty": "^2.0.0",
    "prop-types": "^15.6.1",
    "query-string": "^6.1.0",
    "rc-slider": "^8.6.3",
    "react": "16.5.1",
    "react-animate-height": "^2.0.4",
    "react-css-modules": "^4.7.2",
    "react-dom": "^16.3.2",
    "react-final-form": "^3.6.5",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.1.2",
    "react-redux": "^5.0.7",
    "react-render-html": "^0.6.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-swipeable": "^4.3.0",
    "react-transition-group": "^2.4.0",
    "react-waypoint": "^8.0.3",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-form-saga": "^0.2.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "regenerator-runtime": "^0.12.1",
    "remove-blank-lines": "^1.1.0",
    "reselect": "^3.0.1",
    "serialize-javascript": "^1.5.0",
    "serve-favicon": "^2.5.0",
    "shipit-deploy": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "@babel/preset-stage-0": "^7.0.0-beta.46",
    "asset-require-hook": "^1.2.0",
    "autoprefixer": "^8.4.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-remove-console": "^6.9.1",
    "better-npm-run": "^0.1.0",
    "compression-webpack-plugin": "^1.1.11",
    "cross-spawn": "^6.0.5",
    "css-loader": "^0.28.11",
    "css-modules-require-hook": "^4.2.3",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-minifier": "^3.5.15",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-webpack-plugin": "^2.1.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "opn": "^5.3.0",
    "postcss": "^6.0.22",
    "postcss-loader": "^2.1.4",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.1.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.15",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-manifest-plugin": "^2.0.1"
  },
  "resolutions": {
    "webpack-sources": "^1.1.0"
  }
}
